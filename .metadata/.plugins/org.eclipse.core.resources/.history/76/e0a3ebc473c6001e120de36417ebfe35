package com.project.Java8code;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class TestStudent {

	public static void main(String[] args) {

		List<Student> stu = Arrays.asList(new Student(1, "Akshay", 200, "Pune"),
				new Student(2, "Vaibhav", 400.57, "Pune"), new Student(3, "Abhijeet", 300, "Latur"),
				new Student(4, "Mahesh", 500.67, "Sambhajinagar"), new Student(4, "Jivan", 600, "Washim"));

		List<Student> marksHigherThan300 = stu.stream().filter(s -> s.getSmarks() > 300)
				      .collect(Collectors.toList());
		
		System.out.println("List Of Students having marks greater thyan 300 :"+marksHigherThan300);

	}

}
